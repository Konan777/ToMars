            string sampleInput = "soap{{small}}_{{999}}";
            Regex regex = new Regex(@"\{{.*?\}}");
            MatchCollection matches = regex.Matches(sampleInput);
            foreach (var m in matches)
                Console.WriteLine(m.ToString());
