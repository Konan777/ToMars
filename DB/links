var credentials = SdkContext.AzureCredentialsFactory.FromServicePrincipal(ClientId, ClientSecret, AzureTenantId, AzureEnvironment.AzureGlobalCloud);

        var azure = Microsoft.Azure.Management.Fluent.Azure
            .Configure()
            .Authenticate(credentials)
            .WithSubscription(subscriptionid);
            
https://docs.microsoft.com/ru-ru/azure/active-directory/develop/howto-create-service-principal-portal#get-application-id-and-authentication-key
https://docs.microsoft.com/ru-ru/azure/active-directory/develop/howto-create-service-principal-portal#get-tenant-id

using Microsoft.Azure.Management.Sql.Fluent.Models;

var azure = Microsoft.Azure.Management.Fluent.Azure
                .Authenticate("authfile").WithDefaultSubscription();

            //This retrieves the SQL server from the resourcegroup where the database needs to be created in
            var sqlServer = azure.SqlServers.GetByResourceGroup("resourcegroup", "servername");

            //This database is the template database that is used for creating a new database
            var templatedb = sqlServer.Databases.Get("mytemplatedatabase");

            //Create a new database based on the template database and define your name of your new database
            var database = sqlServer.Databases.Define("mydatabasename")
                .WithSourceDatabase(templatedb)
                .WithMode(CreateMode.Copy)
                .Create();

