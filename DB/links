Set xmlrpc.net service url at runtime

public interface IStateName : IXmlRpcProxy
    {
        [XmlRpcMethod("authenticate")]
        int Authenticate(string db, string username, string password, Dictionary<string, string> parameters);
    }
    public interface IMath : IXmlRpcProxy
    {
        [XmlRpcMethod("authenticate")]
        int Authenticate(string db, string username, string password, Dictionary<string, string> parameters);
    }


