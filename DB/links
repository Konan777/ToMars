    (_common as XmlRpcClientProtocol).Url = "https://maxx.hro.ru/xmlrpc/2/common";

            IStateName proxy1 = XmlRpcProxyGen.Create<IStateName>();
            (proxy1 as XmlRpcClientProtocol).Url = "https://maxx.hro.ru/xmlrpc/2/common";

            var qq = proxy1.Authenticate(_db, _login, _password, null);
