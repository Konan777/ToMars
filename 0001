CREATE TABLE [dbo].[ClbrGraphWS](
	[ClbrGraphWSUid] [uniqueidentifier] NOT NULL,
	[ClbrGraphUid] [uniqueidentifier] NOT NULL,
	[AEUid] [uniqueidentifier] NOT NULL,
	[StdDocUid] [uniqueidentifier] NULL,
	[CompositionUid] [uniqueidentifier] NULL,
	[Name] [nvarchar](150) NOT NULL,
	[Flags] [int] NOT NULL,
 CONSTRAINT [PK_ClbrGraphWS] PRIMARY KEY CLUSTERED 
(
	[ClbrGraphWSUid] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON) ON [PRIMARY]
) ON [PRIMARY]

GO

ALTER TABLE [dbo].[ClbrGraphWS]  WITH CHECK ADD  CONSTRAINT [FK_ClbrGraphWS_AE] FOREIGN KEY([AEUid])
REFERENCES [dbo].[AE] ([AEUid])
GO

ALTER TABLE [dbo].[ClbrGraphWS] CHECK CONSTRAINT [FK_ClbrGraphWS_AE]
GO

ALTER TABLE [dbo].[ClbrGraphWS]  WITH CHECK ADD  CONSTRAINT [FK_ClbrGraphWS_ClbrGraph] FOREIGN KEY([ClbrGraphWSUid])
REFERENCES [dbo].[ClbrGraph] ([ClbrGraphUid])
GO

ALTER TABLE [dbo].[ClbrGraphWS] CHECK CONSTRAINT [FK_ClbrGraphWS_ClbrGraph]
GO

ALTER TABLE [dbo].[ClbrGraphWS]  WITH CHECK ADD  CONSTRAINT [FK_ClbrGraphWS_Composition] FOREIGN KEY([CompositionUid])
REFERENCES [dbo].[Composition] ([CompositionUid])
GO

ALTER TABLE [dbo].[ClbrGraphWS] CHECK CONSTRAINT [FK_ClbrGraphWS_Composition]
GO

ALTER TABLE [dbo].[ClbrGraphWS]  WITH CHECK ADD  CONSTRAINT [FK_ClbrGraphWS_StdDoc] FOREIGN KEY([StdDocUid])
REFERENCES [dbo].[StdDoc] ([StdDocUid])
GO

ALTER TABLE [dbo].[ClbrGraphWS] CHECK CONSTRAINT [FK_ClbrGraphWS_StdDoc]
GO


