CREATE TABLE [dbo].[ClbrGraphSeries](
	[ClbrGraphSeriesUid] [uniqueidentifier] NOT NULL,
	[ClbrGraphUid] [uniqueidentifier] NOT NULL,
	[Name] [nvarchar](150) NOT NULL,
	[SeriesType] [int] NOT NULL,
	[ClbrGraphWSUid] [uniqueidentifier] NULL,
 CONSTRAINT [PK_ClbrGraphSeries] PRIMARY KEY CLUSTERED 
(
	[ClbrGraphSeriesUid] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON) ON [PRIMARY]
) ON [PRIMARY]

GO

ALTER TABLE [dbo].[ClbrGraphSeries]  WITH CHECK ADD  CONSTRAINT [FK_ClbrGraphSeries_ClbrGraph] FOREIGN KEY([ClbrGraphUid])
REFERENCES [dbo].[ClbrGraph] ([ClbrGraphUid])
GO

ALTER TABLE [dbo].[ClbrGraphSeries]  WITH CHECK ADD  CONSTRAINT [FK_ClbrGraphSeries_ClbrGraphWS] FOREIGN KEY([ClbrGraphWSUid])
REFERENCES [dbo].[ClbrGraphWS] ([ClbrGraphWSUid])
GO

ALTER TABLE [dbo].[ClbrGraphSeries] CHECK CONSTRAINT [FK_ClbrGraphSeries_ClbrGraph]
GO




