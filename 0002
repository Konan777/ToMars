
CREATE TABLE [dbo].[ClbrGraphSeriesTest](
	[ClbrGraphSeriesTestUid] [uniqueidentifier] NOT NULL,
	[ClbrGraphSeriesUid] [uniqueidentifier] NOT NULL,
	[TechTestUid] [uniqueidentifier] NOT NULL,
	[Value] [float] NOT NULL,
	[AbsErr] [float] NULL,
 CONSTRAINT [PK_ClbGraphSeriesTest] PRIMARY KEY CLUSTERED 
(
	[ClbrGraphSeriesTestUid] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON) ON [PRIMARY]
) ON [PRIMARY]

GO

ALTER TABLE [dbo].[ClbrGraphSeriesTest]  WITH CHECK ADD  CONSTRAINT [FK_ClbGraphSeriesTest_ClbrGraphSeries] FOREIGN KEY([ClbrGraphSeriesUid])
REFERENCES [dbo].[ClbrGraphSeries] ([ClbrGraphSeriesUid])
GO

ALTER TABLE [dbo].[ClbrGraphSeriesTest] CHECK CONSTRAINT [FK_ClbGraphSeriesTest_ClbrGraphSeries]
GO

ALTER TABLE [dbo].[ClbrGraphSeriesTest]  WITH CHECK ADD  CONSTRAINT [FK_ClbGraphSeriesTest_TechTest] FOREIGN KEY([TechTestUid])
REFERENCES [dbo].[TechTest] ([TechTestUid])
GO

ALTER TABLE [dbo].[ClbrGraphSeriesTest] CHECK CONSTRAINT [FK_ClbGraphSeriesTest_TechTest]
GO


