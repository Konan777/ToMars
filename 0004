GetEqpCheckLogsWithCheckType(allEqpsUids ведет в
Indusoft.LDS.Server.Services.EQP\EqpDataService.cs
  public abstract DataTable GetEqpCheckLogsAllWithCheckType([ParamTypeName("GuidList")]Guid[] eqpUid);


новая версия:
        public Eqp[] GetEqpBySampleTechTest(Guid[] sampleTechTestUids)
        {
            var result = new List<Eqp>();
            if (sampleTechTestUids.Length == 0)
                return result.ToArray();

            BLToolkit.Data.DbManager dbm = DbManager;
            
            var param = new IDbDataParameter[] {
                new SqlParameter()
                {
                    ParameterName = "@SampleTechTestUid"
                    ,SqlDbType = SqlDbType.Structured
                    ,TypeName = "GuidList"
                    ,Value = sampleTechTestUids
                }
            };
            result = dbm.SetCommand(@"SELECT * FROM [dbo].[EqpWithCheckOnSampleDate](@sampleTechTestUid)", param).ExecuteList<Eqp>();
            

            return result.ToArray();
        }
