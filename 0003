C:\Temp\Indus\Source\Indusoft.LDS.Server.Services.Acs\AcsDataService.cs

		public AcsItem[] AcsItem_GetByST(Guid stUid) {
            var sqlQuery = @"
                SELECT ai.AcsItemUid ,
                        ai.AcsPartVerId ,
                        ai.STCPTTUid ,
                        ai.IsActive
                FROM ST 
    	            JOIN STCP ON STCP.STUid = ST.STUid
    	            JOIN STCPT ON STCPT.STCPUid = STCP.STCPUid
    	            JOIN STCPTT ON STCPTT.STCPTUid = STCPT.STCPTUid
    	            JOIN AcsItem ai ON STCPTT.STCPTTUid = ai.STCPTTUid
                WHERE ST.STUid = @stUid";
            return DbManager.SetCommand(sqlQuery, DbManager.Parameter("@stUid", stUid)).ExecuteList<AcsItem>().ToArray();
        }
        public AcsItemEventFreq[] AcsItemEventFreq_GetByST(Guid stUid)
        {
            var sqlQuery = @"
                SELECT f.AcsItemEventFreqId ,
                        f.AcsItemUid ,
                        f.EventFreqUid ,
                        f.Start ,
                        f.Stop ,
                        f.OccurrenceCount ,
                        f.LabelUid ,
                        f.Description
                FROM ST 
    	            JOIN STCP ON STCP.STUid = ST.STUid
    	            JOIN STCPT ON STCPT.STCPUid = STCP.STCPUid
    	            JOIN STCPTT ON STCPTT.STCPTUid = STCPT.STCPTUid
    	            JOIN AcsItem ai ON STCPTT.STCPTTUid = ai.STCPTTUid
    	            JOIN AcsItemEventFreq f ON ai.AcsItemUid = f.AcsItemUid
                WHERE ST.STUid = @stUid";
            return DbManager.SetCommand(sqlQuery, DbManager.Parameter("@stUid", stUid)).ExecuteList<AcsItemEventFreq>().ToArray();
        }
c:\temp\indus\source\indusoft.lds.client.analysisinputcontrol\data\datamanager.cs

using Indusoft.LDS.Services.Contracts.Acss;
		
        private List<STCPTContext> FindSuitableRows(Guid stUid, List<STCPTContext> stcptRows)
        {

			if (SETT=DEFAULT)
				return stcptRows;

            IDataServiceManager dsm = ServiceProvider.GetService<IDataServiceManager>();
            ISTDataService dsacs = dsm.GetService<IAcsDataService>();
	        AcsItem[] AcsItems = dsacs.AcsItem_GetByST(stUid);
            AcsItemEventFreq[] AcsItemEventFreqs = dsacs.AcsItemEventFreq_GetByST(stUid);

			List<Guid> GroupsWithFrequency = new List<Guid>();
			foreach (var stcptRow in stcptRows){
				if (AcsItems.Where(w => w.STCPTTUid==stcptRow.STCPTTUid).Any())
					GroupsWithFrequency.Add(stcpttRow.STCPTT.ParentTTUid);
			}

			List<STCPTContext> suitableRows = new List<STCPTContext>();
			foreach (var stcptRow in stcptRows){
				if (GroupsWithFrequency.Any(a => a==stcptRow.STCPTT.ParentTTUid))
					suitableRows.Add(stcptRow);
				if ((SETT==THREE) && (stcptRow.STCPTT.ParentTTUid==null))
					suitableRows.Add(stcptRow);
			}
			return suitableRows;
        }

        internal string GetFreqDesc(STCPTT stcptt)
        {
            string res = string.Empty;
            foreach (AcsItem ai in AcsItems.FindAll(x => x.STCPTTUid == stcptt.STCPTTUid && !x.IsDeleted))
                foreach (AcsItemEventFreq aief in AcsItemEventFreqs.FindAll(x => x.AcsItemUid == ai.AcsItemUid && !x.IsDeleted))
                {
                    if (string.IsNullOrEmpty(res))
                        res += aief.Description;
                    else
                        res += "; " + aief.Description;
                }
            return res != string.Empty ? res : Resources.STVerDataControlNotCustomized;
        }
