#region AcsItem


Расширяем новыми методами
        Indusoft.LDS.Services.Contracts.Acss->IAcsDataService
        Indusoft.LDS.Server.Services.Acs\AcsDataService.cs
        using Indusoft.LDS.Services.Contracts.Acss;

            private void Some2(Guid stUid)
        {
            IDataServiceManager dsm = ServiceProvider.GetService<IDataServiceManager>();
            IAcsDataService stsrv = dsm.GetService<IAcsDataService>();
            var dt = stsrv.AcsItem_GetAsDataTable(
                sqlQuery = @"
    SELECT ai.AcsItemUid ,
            ai.AcsPartVerId ,
            ai.STCPTTUid ,
            ai.IsActive
    FROM ST 
    	JOIN STCP ON STCP.STUid = ST.STUid
    	JOIN STCPT ON STCPT.STCPUid = STCP.STCPUid
    	JOIN STCPTT ON STCPTT.STCPTUid = STCPT.STCPTUid
    	JOIN AcsItem ai ON STCPTT.STCPTTUid = ai.STCPTTUid
    WHERE ST.STUid = @stUid";
            stPool.AcsItems = DbManager.SetCommand(sqlQuery,
                DbManager.Parameter("@stUid", stUid)).ExecuteList<AcsItem>().ToArray();

            #region AcsItemEventFreq
            sqlQuery = @"
    SELECT f.AcsItemEventFreqId ,
            f.AcsItemUid ,
            f.EventFreqUid ,
            f.Start ,
            f.Stop ,
            f.OccurrenceCount ,
            f.LabelUid ,
            f.Description
    FROM ST 
    	JOIN STCP ON STCP.STUid = ST.STUid
    	JOIN STCPT ON STCPT.STCPUid = STCP.STCPUid
    	JOIN STCPTT ON STCPTT.STCPTUid = STCPT.STCPTUid
    	JOIN AcsItem ai ON STCPTT.STCPTTUid = ai.STCPTTUid
    	JOIN AcsItemEventFreq f ON ai.AcsItemUid = f.AcsItemUid
    WHERE ST.STUid = @stUid";

        }
