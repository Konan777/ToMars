select 
	StdDocVer.StdDocUid as TechUid													-- Методика
	,ae.AEUid, ae.Name as AEName, EntityType.Name as AEType,						-- Объект учета
	AETest.AETestUid, Test.Name as TName, Test.Code as TCode, AETest.IsAttested 	-- Характеристики объекта учета
	--,EngUnit.EngUnitId,  EngUnit.Name, EngUnit.Abbreviation, Dims.WeightEngUnitId	-- Единицы измерения
from ae
inner join aetype on aetype.AEUid=ae.AEUid
inner join EntityType on EntityType.EntityTypeUid=AEType.EntityTypeUid
left join AETest on AETest.AEUid=ae.AEUid
left join Test on Test.TestId=AeTest.TestId
	/*left join EngUnit on EngUnit.EngUnitId=AETest.EngUnitId
	left join CheckProcInfo on CheckProcInfo.EngUnitId=EngUnit.EngUnitId
	left join TechTest on TechTest.TechTestUid=CheckProcInfo.TechTestUid
	left join Dims on Dims.WeightEngUnitId=EngUnit.EngUnitId*/
left join AEStdDoc on AEStdDoc.AEUid=AE.AEUid
left join StdDocVer on StdDocVer.StdDocVerId=AEStdDoc.StdDocVerId
order by aeuid

